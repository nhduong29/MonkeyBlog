<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="monkeyblog" match-name="blog_author"/>
  <table-filter match-catalog="monkeyblog" match-name="blog_category"/>
  <table-filter match-catalog="monkeyblog" match-name="blog_comment"/>
  <table-filter match-catalog="monkeyblog" match-name="blog_post"/>
  <table-filter match-catalog="monkeyblog" match-name="blog_post_to_category"/>
  <table-filter match-catalog="monkeyblog" match-name="blog_related"/>
  <table-filter match-catalog="monkeyblog" match-name="blog_tag"/>
  <table-filter match-catalog="monkeyblog" match-name="blog_user"/>
  <table-filter match-catalog="monkeyblog" match-name="images"/>
</hibernate-reverse-engineering>